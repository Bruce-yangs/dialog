{
  "name": "arale-dialog",
  "version": "1.5.8",
  "description": "Dialog 是通用对话框容器，提供显隐关闭、遮罩层、内嵌iframe、内容区域自定义以及模态对话框等功能。",
  "keywords": [
    "widget",
    "modal",
    "overlay",
    "confirmbox",
    "alert"
  ],
  "author": "偏右 <afc163@gmail.com>",
  "maintainers": [
    "偏右 <afc163@gmail.com>",
    "贯高 <sakura9515@gmail.com>"
  ],
  "homepage": "http://aralejs.org/dialog",
  "repository": {
    "type": "git",
    "url": "git@github.com:aralejs/dialog.git"
  },
  "bugs": {
    "url": "https://github.com/aralejs/dialog/issues"
  },
  "spm": {
    "buildArgs": "--ignore jquery --styleBox"
  },
  "main": "dialog.js",
  "dependencies": {
    "spm-jquery": "1.7.2",
    "arale-overlay": "1.2.0",
    "arale-events": "1.2.0",
    "arale-messenger": "2.1.0"
  },
  "devDependencies": {
    "arale-widget": "1.2.1",
    "spm-expect.js": "0.3.1",
    "spm-sinon": "1.6.0",
    "spm-webpack": "~1.0.0"
  },
  "scripts": {
    "build": "spm-webpack -c"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "publishConfig": {}
}
